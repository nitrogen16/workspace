
public class Switch {

	public static void main(String[] args) {
				
		//1
		int a=6;
		switch(a) {
		case 1: 
			System.out.println("I'm case #1");
			break;
		case 2: 
			System.out.println("I'm case #2");
			break;		
		case 3: 
			System.out.println("I'm case #3");
			break;	
		default:
			System.out.println("I'm a value by default");
		} 
	}

}
