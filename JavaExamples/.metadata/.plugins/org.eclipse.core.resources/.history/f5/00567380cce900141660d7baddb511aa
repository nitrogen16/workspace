
public class IfElse {

	public static void main(String[] args) {
		//1
		int a=59;
		int b=568;
		
		if (a>b) {
				System.out.println("a реально больше b! »т из “–”””””!");
				}
			else {
				System.out.println("a нихрена не больше b! »т из ‘ќќќќЋ—!");
				}; //можно не ставить фигурные скобки, так как одна строка в условии
				
				
				
				//2
				boolean c=true;
				if (c) {
						System.out.println("c из “–”””””!");
						}
					else {
						System.out.println("c из ‘ќќќќЋ—!");
						}; //можно не ставить фигурные скобки, так как одна строка в условии
						

						
						//3
						int f=456;
						int h=568;
						
						if (f>h) {
								System.out.println("f реально больше b! »т из “–”””””!");
								}
							else {
								System.out.println("h нихрена не больше b! »т из ‘ќќќќЋ—!");
								}; //можно не ставить фигурные скобки, так как одна строка в условии


	}

}
