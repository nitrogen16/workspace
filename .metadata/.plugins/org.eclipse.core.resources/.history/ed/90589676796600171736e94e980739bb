package google_search.application;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;

public class Solution {
	static TestFabric testFabric = new TestFabric();
	public static void main(String[] args) throws IOException {
		// TODO Auto-generated method stub
		System.out.println("1 - Chrome\n2 - FireFox");
		BufferedReader reader = new BufferedReader(new InputStreamReader(System.in));
		reader.close();
			String res = reader.readLine();
			int number = Integer.parseInt(res);
			if(number != 1 || number != 2){
				System.out.println("Invalid values. 1 or 2");
			}else{
				if(number == 1){
					Solution sol = testFabric.createTest(BrowserName.CHROME);
				}else if(number == 2) {
					Solution sol = testFabric.createTest(BrowserName.MOZILLA);
				}
			}
	}

}
